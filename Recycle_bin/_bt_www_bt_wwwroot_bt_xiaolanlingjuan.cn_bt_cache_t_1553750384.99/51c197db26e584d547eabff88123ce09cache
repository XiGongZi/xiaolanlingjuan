    var getIndexAjaxNew = function(e,restart){
        wui.uses(["../lazyload.js"],function(){
        if(isGoddsItem) return false;
        /*调用common.js 公用的load动画*/
        $cmsApi.loadAnimation(50);
        var page = $(e).data("page");
        $cmsApi.ajax({
            url:$(e).data("url"),
            data:{                
                page:page,
                cac_id:cac_id,
            }
        }).done(function(item) {
            $cmsApi.loadAnimation(80);
            if(page == 1) $(e).html("");
            /*调用公用的商品列表模板*/
            var tpl = $($cmsApi.goodsListTplTwo(item.data.data,"/index.php?r=p/d",false,"1l")).addClass('listpage'+page);
            $(e).append(tpl);
            cac_id = item.data.cac_id;
            $(e).data("page",page+1);
            $('.listpage'+page + " *[ui-lazyload]").lazyload({
                effect: "fadeIn"
            });
            if(item.data.pageStatus){
                restart();
            }else{
                layer.msg("已经到最后一页了！");
            }
            $cmsApi.loadAnimation(100);
        });
        });
    };

